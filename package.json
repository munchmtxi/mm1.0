{
  "name": "munch-mtxi",
  "version": "1.0.0",
  "description": "A Node.js application for MunchMtxi",
  "main": "index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://https://github.com/munchmtxi/mm1.0"
  },
  "scripts": {
    "audit:log": "powershell Get-Content -Path logs/audit.log -Wait",
    "backup": "node scripts/maintenance.js dailyBackup",
    "dev": "nodemon server.js",
    "format": "prettier --config setup/.prettierrc --write .",
    "lint": "eslint . --config setup/.eslintrc.js",
    "lint:fix": "eslint --fix . --config setup/.eslintrc.js",
    "migrate": "cross-env NODE_ENV=development sequelize db:migrate --config src/config/config.js",
    "migrate:undo": "cross-env NODE_ENV=development sequelize db:migrate:undo --config src/config/config.js",
    "monitor": "NODE_ENV=production node src/config/monitoring.js",
    "prepare": "husky install",
    "prestart": "yarn validate:env && yarn test:connections",
    "seed": "cross-env NODE_ENV=development sequelize db:seed:all --config src/config/config.js",
    "seed:undo": "cross-env NODE_ENV=development sequelize db:seed:undo:all --config src/config/config.js",
    "start": "node server/server.js",
    "validate:env": "node scripts/validateEnv.js",
    "test:connections": "node scripts/testConnections.js"
  },
  "_moduleAliases": {
    "@constants": "src/constants",
    "@socket": "socket",
    "@scripts": "src/scripts",
    "@setup": "server/setup",
    "@config": "./src/config",
    "@controllers": "./src/controllers",
    "@models": "./src/models",
    "@routes": "./src/routes",
    "@utils": "./src/utils",
    "@services": "./src/services",
    "@middleware": "./src/middleware",
    "@validators": "./src/validators",
    "@handlers": "./src/handlers",
    "@serverUtils": "server/utils",
    "@server": "server"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.3.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "africastalking": "^0.7.2",
    "archiver": "^7.0.1",
    "axios": "^1.7.9",
    "bcryptjs": "^3.0.2",
    "bull": "^4.16.5",
    "colors": "1.4.0",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "country-region-data": "^3.1.0",
    "cross-env": "^7.0.3",
    "csurf": "^1.11.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "event-loop-stats": "^1.4.1",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.0",
    "express-status-monitor": "^1.3.4",
    "express-validator": "^7.2.1",
    "express-xss-sanitizer": "^2.0.0",
    "google-libphonenumber": "^3.2.40",
    "helmet": "^8.0.0",
    "http": "^0.0.1-security",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "joi-password-complexity": "^5.2.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.3",
    "papaparse": "^5.5.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "password-validator": "^5.3.0",
    "pdfkit": "^0.16.0",
    "perf_hooks": "^1.0.0",
    "pg": "^8.13.3",
    "pg-hstore": "^2.3.4",
    "qrcode": "^1.5.4",
    "rate-limit-redis": "^4.2.0",
    "rate-limiter-flexible": "^2.4.1",
    "redis": "^4.6.7",
    "sequelize": "^6.37.5",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "speakeasy": "^2.0.0",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^5.4.5",
    "ua-parser-js": "^2.0.2",
    "uuid": "^11.0.5",
    "web-push": "^3.4.5",
    "whatsapp-web.js": "^1.19.5",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xlsx": "^0.18.5",
    "xss": "^1.0.14",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@babel/preset-env": "^7.26.7",
    "@types/google-libphonenumber": "^7.4.30",
    "autoprefixer": "^10.4.20",
    "babel-jest": "^29.7.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-security": "^3.0.1",
    "glob": "8.1.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.1",
    "madge": "5.0.1",
    "nodemon": "^3.1.9",
    "postcss": "^8.5.3",
    "prettier": "^3.4.2",
    "sequelize-cli": "^6.6.2",
    "supertest": "^7.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
